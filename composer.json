{
    "name": "laraflock/multi-tenant",
    "description": "Multi tenant laravel lts with separation between tenant and system",
    "keywords": ["laravel", "multi-tenant", "multi-tenancy", "laraflock", "hyn"],
    "license": "MIT",
    "authors": [
        {
            "name": "DaniÃ«l `Luceos` Klabbers",
            "email": "daniel+hyn@klabbers.email"
        }
    ],
    "support": {
        "issues": "https://github.com/laraflock/multi-tenant/issues",
        "source": "https://github.com/laraflock/multi-tenant",
        "docs": "http://hyn.me"
    },
    "require": {
        "php": ">=5.5.9",
        "hyn/framework": "1.*",
        "hyn/webserver": "1.*"
    },
    "suggest": {
        "hyn/management-interface": "Provides a user interface for managing multi tenancy"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Laraflock\\MultiTenant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laraflock\\MultiTenant\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
